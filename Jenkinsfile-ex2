     steps {
      sh '''
       zip --version
       unzip --version
       [ -f /tmp/zip_job.sh ] && echo "zip_job.sh EXIST at /tmp ." || echo "FILE does not exist." 
       cat /etc/centos-release
       '''
       }
      }
    }
  }
    
