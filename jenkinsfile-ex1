pipeline {
   agent any
    stages {
        stage('test') {
            steps {
                sh 'echo "hello"'
            }
           }
         stage ('test script') {
         agent { label 'master' }
            steps {
         sh 'echo "Hello test2" '
             }
            }
          stage ('test script') { 
            agent { dockerfile true} 
           steps {
           sh 'echo "~~~~ running dockerfile as an agent ~~~~~ "'
           }
           }
         }
      }
      

