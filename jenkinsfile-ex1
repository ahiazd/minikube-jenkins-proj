pipeline {
    agent {
      docker { 
                image 'cetnos7-ahiaz:latest'
             }
        
      stages {
        stage('Test agent') {
            steps {
                sh 'echo "Hello world!" ' 
            }
        }
        
        stage ('Check OS') {
         steps {
         sh ' cat /etc/release-os'
         }
        }   
    }
}
   

