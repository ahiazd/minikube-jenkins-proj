pipeline {
   agent {label 'docker-slave' }
    stages {
        stage('test') {
            steps {
                sh 'echo "hello"'
            }
           }
         stage ('test script') {
         agent { dockerfile true }
            steps {
          sh 'echo "Hello test2" '
             }
           }
          agent { dockerfile} 
           steps {
           sh 'echo "~~~~ running dockerfile as an agent ~~~~~ "'
           }
         }
      }
      

